{
    "name": "torre/api",
    "description": "Torre api",
    "keywords": [
        "torre",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "uzma",
            "email": "author_email@example.com",
            "homepage": "http://author-website.example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Torre\\": "src/",
            "Torre\\Service\\": "src/services/",
            "Torre\\Views\\": "src/views/",
            "Torre\\Controllers\\": "src/controllers/",
            "Torre\\Factory\\": "src/factory/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Torre\\Api\\Test\\": "tests"
        }
    },
  "repositories": [
    {
      "type": "path",
      "url": "src"
    }
  ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
